{
  "name": "soundcloud",
  "version": "1.9.5",
  "description": "SoundCloud Desktop App",
  "main": "init.js",
  "scripts": {
    "build": "node .build/build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxcnoname666/SoundCloud-Desktop.git"
  },
  "author": "zxcnoname666",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zxcnoname666/SoundCloud-Desktop/issues"
  },
  "homepage": "https://github.com/zxcnoname666/SoundCloud-Desktop#readme",
  "devDependencies": {
    "electron": "^31.3.0"
  },
  "dependencies": {
    "https-proxy-agent": "^5.0.1",
    "node-fetch": "^2.6.12",
    "notify-manager-electron": "^1.5.0",
    "qurre-socket": "^1.1.0",
    "socks-proxy-agent": "^8.0.2"
  },
  "build": {
    "files": [
      "!.build/*",
      "!.git/*",
      "!dist/*",
      "!update_info.json"
    ],
    "appId": "SoundCloud",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "icons/appLogo.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "zip"
      ],
      "icon": "icons/appLogo.png",
      "category": "Audio"
    }
  }
}
