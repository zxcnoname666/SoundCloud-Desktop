{
  "name": "soundcloud",
  "version": "4.1.1",
  "description": "SoundCloud Desktop App",
  "type": "module",
  "main": "dist/init.js",
  "scripts": {
    "dev": "tsx build.ts && electron dist/init.js --dev",
    "dev:watch": "tsx build.ts --skip-copy && electron dist/init.js --dev",
    "build": "pnpm run build:production && node scripts/forge-make.cjs",
    "build:app": "tsx build.ts",
    "build:production": "tsx build.ts --production",
    "package": "node scripts/forge-package.cjs",
    "make": "node scripts/forge-make.cjs",
    "start": "electron dist/init.js",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx --fix",
    "biome:check": "biome check .",
    "biome:write": "biome check --write .",
    "format": "pnpm biome:write",
    "prepare": "simple-git-hooks"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.23.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zxcnoname666/SoundCloud-Desktop.git"
  },
  "author": "zxcnoname666",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zxcnoname666/SoundCloud-Desktop/issues"
  },
  "homepage": "https://github.com/zxcnoname666/SoundCloud-Desktop#readme",
  "simple-git-hooks": {
    "pre-commit": "pnpm run format"
  },
  "pnpm": {
    "overrides": {
      "js-yaml@<4.1.1": ">=4.1.1",
      "glob@>=10.2.0 <10.5.0": ">=10.5.0"
    },
    "ignoredBuiltDependencies": [
      "@biomejs/biome",
      "esbuild",
      "register-scheme"
    ],
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "simple-git-hooks"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.3",
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-appx": "^7.10.2",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-dmg": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@eslint/js": "^9.39.1",
    "@reforged/maker-appimage": "^5.1.0",
    "@types/discord-rpc": "^4.0.10",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "electron": "^38.7.1",
    "esbuild": "^0.27.0",
    "eslint": "^9.39.1",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "json5": "^2.2.3",
    "marked": "^17.0.1",
    "monaco-editor": "^0.55.1",
    "node-fetch": "^3.3.2",
    "qurre-socket": "^1.1.0"
  }
}
